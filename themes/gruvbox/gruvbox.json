{
	"colors": {
		"settings": {
			"color_mode": "pywal"
		}
	},


	"i3wm": {
		"font": "pango:JetBrainsMono 10",
		"terminal": "gnome-terminal",
		"colors": {
			"background": ["background"],
			"focused": ["foreground", "background", "foreground", "color12", "color12"],
			"unfocused": ["foreground", "background", "foreground", "color4", "color4"],
			"focused_inactive": ["foreground", "background", "foreground", "color4", "color4"],
			"urgent": ["foreground", "background", "foreground", "color4", "color4"],
			"placeholder": ["foreground", "background", "foreground", "color4", "color4"]
		},

		"bindsyms": {
			"$mod+Shift+space": "floating enable, border normal 10",
			"$mod+Control+space": " floating disable, border none"
		}
	},

	"polybar": {
		"bar/main": {
		"modules-left": "xworkspaces",
		"modules-center": "",
		"modules-right": "pulseaudio memory cpu date",
		"border-size": "0pt"
		}
	},
	

	"vimrc": {
		"colorscheme": "gruvbox"
	},
	"wallpaper":  "gruvbox-tux.png"
}
