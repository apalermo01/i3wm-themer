{
    "settings": {
        "use_pywal": true,
    "config": "config.yaml",
    "install": "./defaults/"
    },

    "xresources": {},

    "i3wm": {
        "use_xresources": true,
		"font": "pango:JetBrainsMono 10",
		"terminal": "gnome-terminal"
    },

    "polybar": {
        "use_xresources": true,
        "modules-left": "xworkspaces",
        "modules-center": "date",
        "modules-right": "wlan eth powermenu"
    },

	"bashrc": {
		"pywal_colors": true,
		"git_onefetch": true,
		"extra_lines": ["cbonsai -p"]

	},
    "wallpaper": {
		"method": "feh",
		"name": "trees.jpg"
	}
}

